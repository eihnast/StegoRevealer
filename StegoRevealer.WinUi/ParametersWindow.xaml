<Window x:Name="Window" x:Class="StegoRevealer.WinUi.ParametersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StegoRevealer.WinUi"
        mc:Ignorable="d"
        Height="560" Width="800"
        Title="Stego Revealer" Background="#FF1D1D1D" 
        ScrollViewer.VerticalScrollBarVisibility="Disabled" ResizeMode="CanMinimize" Icon="/ProjectIcon.png"
        WindowStartupLocation="CenterOwner">
    
    <Grid x:Name="RootGrid">
        <ContentControl x:Name="RootContentControl" HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Stretch"/>
    </Grid>
    
</Window>
